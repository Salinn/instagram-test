
<a href="/auth/facebook" class="auth_provider">
  <%= image_tag 'https://lh5.ggpht.com/_XklY7dK6yGsYt53X15RIp7-tbdjWMwQwl_iUAcW-uSq_8zAprmywdn5DQfBHvxZurs1=w300', :size => "64x64" %>
</a>
<a href="/auth/instagram" class="auth_provider">
  <%= image_tag "https://pbs.twimg.com/profile_images/1550954462/instagramIcon_400x400.png", :size => "64x64" %>
</a>
<hr>
<hr>
<h1>Test</h1>
<% unless true %>
    <h1>
      <%pa = @facebook.get_object("me") %>
      <%con = @facebook.get_connections("me","photos/uploaded") %>
      <%= pa['email'] %>
      <br />
      <br />
      <%= pa['first_name'] %>
      <%= pa['last_name'] %>
      <br />
      <br />
      <%con.each do |c| %>
          <%= image_tag c['images'].last['source']  %>
      <% end %>
    </h1>
<% end %>

<hr>
<hr>

<% unless Authentication.where(provider: "instagram").empty? %>
    <% @image = Image.new %>
    <% @instagram.each do |instagram| %>
        <div style="float: left; padding-right: 5px" align="center">
          <%= render 'images/select_image', instagram: instagram %>
        </div>
    <% end %>

<% end %>
<br />
<br />
<br />